<!doctype html>
<html lang="en-US">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{message}}</title>
    <meta name="description" content="{{message}}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="/apple-touch-icon.png">
    <link rel="stylesheet" href="/style/main.css">
</head>
<body>

{{#if json}}
    <script type="application/json" id="json">{{{json}}}</script>
{{/if}}

<script src="/socket.io/socket.io.js"></script>
<script>
    var socket = io();
</script>

<script type="text/javascript" src="/vendor/spin.js"></script>
<script type="text/javascript" src="/js/vendor.min.js"></script>



{{#if user}}
    <script type="application/json" id="user" >{{{user}}}</script>
{{/if}}

<section style="height: 0; width: 0; overflow:hidden;" id="page-content">
    <h1>{{status}}</h1>
    <p>{{message}}</p>
</section>


{{#if js}}

    <main id="{{js_id}}">{{{json}}}</main>
    <script src="/js/{{js}}.min.js"></script>
{{/if}}

<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-17944087-2', 'auto');
    ga('send', 'pageview');
</script>

</body>
</html>


